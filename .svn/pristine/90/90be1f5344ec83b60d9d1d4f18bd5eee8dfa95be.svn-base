
@font-face {font-family: "icon-min";
    src: url(data:font/ttf;charset=utf-8;base64,) format('truetype');
}
.ui-icon-close,
.ui-icon-search{
    @include icon;
    font-size: 16px;
    color: #777;
}
.ui-icon-close:before {
    content: $icon-close;
}

.ui-icon-search:before { content: $icon-search;}
