<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <title>QUI Demo</title>
    <script type="text/javascript" src="http://tajs.qq.com/stats?sId=37342703" charset="UTF-8"></script>
    <link rel="stylesheet" href="../css/global.css">
    <link rel="stylesheet" href="./demo.css">
    <link rel="stylesheet" href="./demo_v2.css">
    <script src="../lib/zepto.min.js"></script>
</head>

<body ontouchstart>
        <!-- <h1>按钮 btn</h1> -->

    <!-- <footer class="ui-footer ui-footer-btn">
        <ul class="ui-tiled ui-border-t">
            <li data-href="index.html" class="ui-border-r">
                <div>基础样式</div>
            </li>
            <li data-href="ui.html" class="ui-border-r">
                <div>UI组件</div>
            </li>
            <li data-href="js.html">
                <div>JS插件</div>
            </li>
        </ul>
    </footer> -->
    <section class="ui-container">

<section id="btn">
    <h1 class="title">BUTTON</h1>

    <!--  白色按钮 -->

    <div class="demo-item">
        <!-- <p class="demo-desc">普通小按钮(样式1)</p> -->
        <div class="demo-block">
            <div class="ui-btn-wrap">
                <!-- 按钮样式1 -->
                <button class="ui-btn">
                    确定
                </button>
                <button class="ui-btn disabled">
                    确定
                </button>
            </div>
        </div>
    </div>

    <!-- 进度的按钮 -->
    <div class="demo-item">
        <div class="demo-block">
            <div class="ui-btn-wrap">
                <button class="ui-btn ui-btn-progress">
                    <span>下载</span>
                    <span class="ui-btn-inner" style="width:50%"><span>下载</span></span>
                </button>
                 <button class="ui-btn ui-btn-progress disabled">
                    <span>下载</span>
                    <span class="ui-btn-inner" style="width:50%"><span>下载</span></span>
                </button>
            </div>
        </div>
    </div>

    <!-- 蓝色按钮 -->
    <div class="demo-item">
        <div class="demo-block">
            <div class="ui-btn-wrap">
                <button class="ui-btn ui-btn-primary">
                    确定
                </button>
                <button class="ui-btn disabled" >
                   确定
                </button>
            </div>
        </div>
    </div>

    <!-- 红色按钮 -->
    <div class="demo-item">
        <div class="demo-block">
            <div class="ui-btn-wrap">
                <button class="ui-btn ui-btn-danger">
                    确定
                </button>
                  <button class="ui-btn ui-btn-danger disabled">
                   确定
                </button>
            </div>
        </div>
    </div>

    <!-- 高亮按钮  -->
      <div class="demo-item color-black">
        <div class="demo-block">
            <div class="ui-btn-wrap">
                <button class="ui-btn ui-btn-highlight">
                    确定
                </button>
                <button class="ui-btn ui-btn-highlight disabled">
                    确认
                </button> 
            </div>
        </div>
    </div>


    <div class="demo-item">
        <div class="demo-block">
            <div class="ui-btn-wrap">
                <button class="ui-btn-lg">
                    确定
                </button>
            </div>
            <div class="ui-btn-wrap">
                <button class="ui-btn-lg ui-btn-primary">
                    确定
                </button>
            </div>
            <div class="ui-btn-wrap">
                <button class="ui-btn-lg ui-btn-danger">
                    确定
                </button>
            </div>
            <div class="ui-btn-wrap">
                <button class="ui-btn-lg disabled">
                    取消
                </button>
            </div>
             <div class="ui-btn-wrap color-black">
                <button class="ui-btn-highlight ui-btn-lg">
                    取消
                </button>
            </div>
        </div>
    </div>

    <!-- 2/3的按钮 -->
    <div class="demo-item">
        <div class="demo-block">
            <div class="ui-btn-wrap ui-btn-wrap-middle">
                <button class="ui-btn-lg-nowhole">
                    取消
                </button>
            </div>
            <div class="ui-btn-wrap ui-btn-wrap-middle">
                <button class="ui-btn-lg-nowhole ui-btn-primary">
                    取消
                </button>
            </div>
            <div class="ui-btn-wrap ui-btn-wrap-middle">
                <button class="ui-btn-lg-nowhole ui-btn-danger">
                    取消
                </button>
            </div>
            <div class="ui-btn-wrap ui-btn-wrap-middle">
                <button class="ui-btn-lg-nowhole ui-btn-danger disabled">
                    取消
                </button>
            </div>
        </div>
    </div>
   

    

    
    
    
    
</section>

        </section><!-- /.ui-container-->
        <script>
        $('.ui-list li,.ui-tiled li').click(function(){
            if($(this).data('href')){
                location.href= $(this).data('href');
            }
        });
        $('.ui-header .ui-btn').click(function(){
            location.href= 'index_v2.html';
        });
        </script>
    </body>
</html>
