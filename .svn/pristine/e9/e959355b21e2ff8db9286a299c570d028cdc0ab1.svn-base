$positive-bg:           #18b4ed;
$header-height:         45px;
$mask-bg:               rgba(0,0,0,.3);
$input-wrap-bg:         #ebeced;

//txt
//-----------------------------

$txt-default:           #000;
$txt-info:              #777;
$txt-muted:             #bbb;
$txt-warning:           #ff4222;
$txt-highlight:         #ff8444;
$txt-link:              #00a5e0;
$txt-feeds:             #314c83;

//lineheight
// ---------------------------------
$line-height-base:      1.5;
$line-height-s:         1.3;
$line-height-txt:       24px;
$line-height-bar:       46px;
$line-height-title:     55px;
$txt-padding-top:       10px;
