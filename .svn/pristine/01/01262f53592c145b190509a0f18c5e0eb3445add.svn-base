html{
    width: 100%;
    /*overflow: hidden;*/
}
body{
  -webkit-user-select:initial;
  background-color: #f8f8f8;
  width: 100%;
  /*overflow: hidden;*/
}
h1 {
    font-size: 40px;
    color: #000;
    padding-top: 55px;
    text-align: center;
}
.index-wrap h1{
    font-size: 40px;
    color: #34C0F5;
    padding-top: 0;
}
.index-wrap h2{
    font-size: 12px;
    color: #bbb;
    line-height: 1.2;
}
.index-wrap{
    width: 100%;
    overflow-x: hidden;
}
.index-wrap .header{
    background-color: #F7F7F7;
    height: 135px;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-align: center;
    -webkit-box-pack: center;
}
.index-wrap .footer{
    height: 55px;
    display: -webkit-box;
    -webkit-box-align: center;
    -webkit-box-pack: center;
    background-color: #F7F7F7;
}
.logo{
    width: 53px;
    height: 15px;
    background: url(img/isux.png) no-repeat;
    background-size: 100% auto;
}
.index-wrap table{
    width: 100%;
    overflow: hidden;
}
.demo-block td{
    width: 33.33%;
    /*padding-top: 33.33%;*/
    position: relative;
}
.demo-block td>div{
    padding: 23px 0 23px 0;
    background: #fff;
}
.demo-block td>div:active{
    background: #e4e4e4;
}
.demo-block .tit{
    text-align: center;
    color: #000;
    font-size: 16px;
}
.demo-block .icon{
    width: 46px;
    height: 46px;
    margin: 0 auto;
    /*margin-bottom: 5px;*/
}
